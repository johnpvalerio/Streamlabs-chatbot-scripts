{
    "output_file": "config.json",

    "liveOnly": {
        "type": "checkbox",
        "value": true,
        "label": "Live Only Mode",
        "tooltip": "True if you want it to only run when you are live",
        "group" : "General"
    },
    "command": {
        "type": "textbox",
        "value": "!roll",
        "label": "Command",
        "tooltip": "Name of the command",
        "group": "General"
    },
    "alias": {
        "type": "textbox",
        "value": "!dice",
        "label": "Alias",
        "tooltip": "Name of the alias. (Usually a short call for the command)",
        "group": "General"
    },
    "numDiceCap": {
        "type": "numberbox",
        "label": "Max number of dice",
        "value": 10,
        "tooltip": "Exceeding 10 might clog up the chat and longer to execute",
        "group": "General"
    },
    "typeDiceCap": {
        "type": "numberbox",
        "label": "Max dice result number",
        "value": 1000,
        "tooltip": "Exceeding 1000 might clog up the chat",
        "group": "General"
    },
    "useCooldown": {
        "type": "checkbox",
        "value": true,
        "label": "User Cooldowns",
        "tooltip": "Enable/Disable user cooldown",
        "group": "Cooldown"
    },
    "userCdMin": {
        "type": "numberbox",
        "label": "Cooldown (minutes)",
        "value": 10,
        "tooltip": "Cooldown in minutes.",
        "group": "Cooldown"
    },
    "userCdSec": {
        "type": "numberbox",
        "label": "Cooldown (seconds)",
        "value": 0,
        "tooltip": "Cooldown in seconds.",
        "group": "Cooldown"
    }
}
