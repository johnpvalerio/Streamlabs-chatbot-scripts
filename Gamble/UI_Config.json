{
    "output_file": "config.json",

    "liveOnly": {
        "type": "checkbox",
        "value": true,
        "label": "Live Only Mode",
        "tooltip": "True if you want it to only run when you are live",
        "group" : "General"
    },
    "command": {
        "type": "textbox",
        "value": "!gamble",
        "label": "Command",
        "tooltip": "Name of the command",
        "group": "General"
    },
    "alias": {
        "type": "textbox",
        "value": "!g",
        "label": "Alias",
        "tooltip": "Name of the alias. (Usually a short call for the command)",
        "group": "General"
    },
    "useCooldown": {
        "type": "checkbox",
        "value": true,
        "label": "Use Cooldowns",
        "tooltip": "Enable/Disable user cooldown",
        "group": "Cooldown"
    },
    "userCdMin": {
        "type": "numberbox",
        "label": "Cooldown (minutes)",
        "value": 10,
        "tooltip": "Cooldown in minutes.",
        "group": "Cooldown"
    },
    "userCdSec": {
        "type": "numberbox",
        "label": "Cooldown (seconds)",
        "value": 0,
        "tooltip": "Cooldown in seconds.",
        "group": "Cooldown"
    },
     "rewardMul": {
        "type": "numberbox",
        "value": 1,
        "label": "Winning multiplier",
        "tooltip": "Set the winning multiplier.",
        "group": "Currency"
    },
    "winThresh": {
        "type": "numberbox",
        "value": 50,
        "label": "Threshold",
        "tooltip": "Set the winning threshold number. Must win above this number.",
        "group": "Currency"
    },
    "useCapBet": {
        "type": "checkbox",
        "value": false,
        "label": "Use bet cap",
        "tooltip": "Enable/Disable maximum bet",
        "group": "Currency"
    },
    "capBet": {
        "type": "numberbox",
        "value": 0,
        "label": "Max bet amount",
        "tooltip": "Set the maximum amount of points to wager",
        "group": "Currency"
    }
}
